         <script contentType="application/x-javascript" name="localLib">
function podpisConcatenate (objOsoba) {

	
	var podpis = "";
	if (objOsoba == null) 
		podpis = null
	else
		podpis = (objOsoba.titul_pred.rawValue == null?"":objOsoba.titul_pred.rawValue) + 
		(objOsoba.jmeno.rawValue == null?"":objOsoba.jmeno.rawValue) + 
		(objOsoba.prijmeni.rawValue == null?"":objOsoba.prijmeni.rawValue) + 
		(objOsoba.titul_za.rawValue == null?"":objOsoba.titul_za.rawValue);
	
	return podpis;
}

function dalsi_skutecnosti_init(forma_upadku, rozhodnuti_o_upadku, spojeni ) {
	if (forma_upadku == 1 &amp;&amp; rozhodnuti_o_upadku == 1 &amp;&amp; spojeni== 1)
		sharedLib.visibleIfHidden(Formular.popis_rozhodujicich_skutecnosti.wrapper.dalsi_skutecnosti.dalsi_skutecnosti)
	else  
		sharedLib.hideIfVisible(Formular.popis_rozhodujicich_skutecnosti.wrapper.dalsi_skutecnosti.dalsi_skutecnosti);
};

function dluznikPodnikatel(dluznik) {
	if (dluznik.wrapper._fyzicka_osoba.count &gt; 0) {
		if (dluznik.wrapper.resolveNode("fyzicka_osoba[0].udaje_osoba_wrapper.udaje_osoba.ico").rawValue != null) {
			return true;
		}
		
		if (dluznik.wrapper._fyzicka_osoba.count &gt; 1 &amp;&amp; dluznik.wrapper.resolveNode("fyzicka_osoba[1].udaje_osoba_wrapper.udaje_osoba.ico").rawValue != null) {
			return true;
		}
	} else if (dluznik.wrapper._pravnicka_osoba.count &gt; 0) {
		return true;
	}
	return false;
}
