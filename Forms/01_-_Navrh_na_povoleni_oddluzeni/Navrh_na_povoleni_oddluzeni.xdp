		sharedLib.setOnValueChange(formObj.zahranicni_veritele.wrapper.zahr_veritele.text, texts.sec11_sublabel);
	}	
}

function sec5_popis_caption(dalsi_skut, upadek, dluznik, spojeni, forma_upadku, formObjpopis) {
	if (formObjpopis == null)
