function setFormTitle(ins_navrh, formObj) {
	if (formObj == null)
		return;
		
	sharedLib.setOnValueChange(formObj.title.title, ins_navrh == "1" ? texts.form_title_ins : texts.form_title);
}

function setFromUpadek(upadek, dalsi_skut, formObj) {
	if (formObj == null)
		return;		
		
	if (upadek == "2") {		
		sharedLib.setOnValueChange(formObj.popis_rozhodujicich_skutecnosti.wrapper.label, formObj.druh_navrhu.wrapper.spolecny_navrh.spolecny_navrh.rawValue == "1" ? texts.sec5_multi_2 : texts.sec5_2);
		sharedLib.setOnValueChange(formObj.popis_rozhodujicich_skutecnosti.wrapper.zavazky.label, texts.sec5_sublabel_2);
		
	} else {		
		sharedLib.setOnValueChange(formObj.popis_rozhodujicich_skutecnosti.wrapper.label, formObj.druh_navrhu.wrapper.spolecny_navrh.spolecny_navrh.rawValue == "1" ? texts.sec5_multi : texts.sec5);
		sharedLib.setOnValueChange(formObj.popis_rozhodujicich_skutecnosti.wrapper.zavazky.label, texts.sec5_sublabel);
	}
	labels.sec5_popis_caption(
		Formular.popis_rozhodujicich_skutecnosti.wrapper.dalsi_skutecnosti.dalsi_skutecnosti.rawValue,
		Formular.druh_navrhu.wrapper.spojeni_ano.rozhodnuti_o_upadku.rawValue,
		Formular.dluznik,
		Formular.druh_navrhu.wrapper.spojeni.spojeni.rawValue,
		Formular.popis_rozhodujicich_skutecnosti.wrapper.forma_upadku.forma_upadku.rawValue,
		Formular.popis_rozhodujicich_skutecnosti.wrapper.dalsi_skutecnosti.popis);
}

function setFromSpolecnyNavrh(spolecny_navrh, formObj) {
	if (formObj == null)
		return;		
		
	if (spolecny_navrh == "1") {		
		sharedLib.setOnValueChange(formObj.dluznik.wrapper.label, texts.sec3_multi);
		sharedLib.setOnValueChange(formObj.osoby_jednajici_za_dluznika.wrapper.label, texts.sec4_multi);
		sharedLib.setOnValueChange(formObj.osoby_jednajici_za_dluznika.wrapper.osoby_switch.text, texts.sec4_multi_sublabel);
		
		if (formObj.druh_navrhu.wrapper.spojeni_ano.rozhodnuti_o_upadku != null &amp;&amp; formObj.druh_navrhu.wrapper.spojeni_ano.rozhodnuti_o_upadku.rawValue == "2")
			sharedLib.setOnValueChange(formObj.popis_rozhodujicich_skutecnosti.wrapper.label, texts.sec5_multi_2);
		else
			sharedLib.setOnValueChange(formObj.popis_rozhodujicich_skutecnosti.wrapper.label, texts.sec5_multi);
			
		sharedLib.setOnValueChange(formObj.okolnosti_pro_vysi_plneni.wrapper.label, texts.sec6_multi);
		sharedLib.setOnValueChange(formObj.prijmy.wrapper.label, texts.sec7_multi);
		sharedLib.setOnValueChange(formObj.prijmy_ocekavane.wrapper.label, texts.sec8_multi);
		sharedLib.setOnValueChange(formObj.drivejsi_rizeni.wrapper.label, texts.sec9_multi);
		sharedLib.setOnValueChange(formObj.drivejsi_rizeni.wrapper.drivejsi_rizeni.text, texts.sec9_multi_sublabel);
		sharedLib.setOnCaptionChange(formObj.navrh_na_zmenu_splatky.wrapper.navrh_sekce.duvody_upadku_wrapper.duvody_upadku, texts.sec10_multi_duvody_upadku_caption);
		sharedLib.setOnValueChange(formObj.zahranicni_veritele.wrapper.zahr_veritele.text, texts.sec11_multi_sublabel);
	} else {
		sharedLib.setOnValueChange(formObj.dluznik.wrapper.label, texts.sec3);
		sharedLib.setOnValueChange(formObj.osoby_jednajici_za_dluznika.wrapper.label, texts.sec4);
		sharedLib.setOnValueChange(formObj.osoby_jednajici_za_dluznika.wrapper.osoby_switch.text, texts.sec4_sublabel);
		
		if (formObj.druh_navrhu.wrapper.spojeni_ano.rozhodnuti_o_upadku != null &amp;&amp; formObj.druh_navrhu.wrapper.spojeni_ano.rozhodnuti_o_upadku.rawValue == "2")
			sharedLib.setOnValueChange(formObj.popis_rozhodujicich_skutecnosti.wrapper.label, texts.sec5_2);
		else
			sharedLib.setOnValueChange(formObj.popis_rozhodujicich_skutecnosti.wrapper.label, texts.sec5);
			
		sharedLib.setOnValueChange(formObj.okolnosti_pro_vysi_plneni.wrapper.label, texts.sec6);
