function calcVyse1(pocMes) { //vypocet pro pole "Splátkový kalendář se zpeněžením majetkové podstaty" / vyse
	var splatka =  calcSplatka ();
	var r = 0.0 ;
	if (splatka != 0) {
		r =  (
			(
				calcSplatka () - getOdm_ins_spr ()) * pocMes 
				+ getNumber (Zprava_pro_oddluzeni.ocekavany_vytezek.wrapper.splatky_a_majetek_wrapper.splatky_a_majetek)
				- getOdm_prihl ()
				- getNumber (Zprava_pro_oddluzeni.vyzivovaci_povinnost.wrapper.dluznici.dluznik1.vyzivne_dluzne)
				- (getNumber (Zprava_pro_oddluzeni.vyzivovaci_povinnost.wrapper.dluznici.dluznik1.vyzivne.vyzivne_celkova_vyse) * pocMes)
				- getNumber (Zprava_pro_oddluzeni.vyzivovaci_povinnost.wrapper.dluznici.dluznik2.vyzivne_dluzne)
				- (getNumber (Zprava_pro_oddluzeni.vyzivovaci_povinnost.wrapper.dluznici.dluznik2.vyzivne.vyzivne_celkova_vyse) * pocMes)
				- calcDPH( getNumber (Zprava_pro_oddluzeni.ostatni.wrapper.odmena_sepsani.vyse_neuhrazeno), getNumber (Zprava_pro_oddluzeni.ostatni.wrapper.zpracovatel_platcem_dph_wrapper.zpracovatel_platcem_dph.zpracovatel_platcem_dph))
			)
		}
	else r = "";
	if (r != "" &amp;&amp; r &lt; 0) r = 0;
	return r ;
	
};

function calcMira1(pocMes) { //vypocet pro pole "Splátkový kalendář se zpeněžením majetkové podstaty" / mira
	var r = "";
	var vyse = calcVyse1(pocMes);
	if (vyse != 0 ) r = 100 * vyse  / getNumber (Zprava_o_prezkumu.wrapper.celkem_bez_podrizenych_wrapper.celkem_bez_podrizenych);
	if (r &gt; 999.9999) {
		r = 999.9999;
	};
	if (r &lt; 0) {
		r = 0;
	};
	return r;

};

function calcPocMes1() { //Výpočet pro výslednou hodnotu v poli B102, 103, 104 (spojeno do jednoho pole) „počet měsíců nutných ke 100% uspokojení pohledávek“
	var r = "";
	var splatka = calcSplatka ();
	if (getNumber (Zprava_o_prezkumu.wrapper.celkem_vcetne_podrizenych_wrapper.celkem_vcetne_podrizenych) != 0 &amp;&amp; splatka  != 0)
	r = (
				(
					getNumber (Zprava_o_prezkumu.wrapper.celkem_vcetne_podrizenych_wrapper.celkem_vcetne_podrizenych)
					+ getOdm_prihl ()	
					+ getNumber (Zprava_pro_oddluzeni.vyzivovaci_povinnost.wrapper.dluznici.dluznik1.vyzivne_dluzne)
					+ getNumber (Zprava_pro_oddluzeni.vyzivovaci_povinnost.wrapper.dluznici.dluznik2.vyzivne_dluzne)
					- getNumber (Zprava_pro_oddluzeni.ocekavany_vytezek.wrapper.splatky_a_majetek_wrapper.splatky_a_majetek)
					+ calcDPH( getNumber (Zprava_pro_oddluzeni.ostatni.wrapper.odmena_sepsani.vyse_neuhrazeno), getNumber (Zprava_pro_oddluzeni.ostatni.wrapper.zpracovatel_platcem_dph_wrapper.zpracovatel_platcem_dph.zpracovatel_platcem_dph))

				)
				/
				( splatka  
				- getOdm_ins_spr ()
				- getNumber (Zprava_pro_oddluzeni.vyzivovaci_povinnost.wrapper.dluznici.dluznik1.vyzivne.vyzivne_celkova_vyse)
				- getNumber (Zprava_pro_oddluzeni.vyzivovaci_povinnost.wrapper.dluznici.dluznik2.vyzivne.vyzivne_celkova_vyse)	
				)
			);
	if (r &lt; 0) r = 0;
	return Math.ceil(r);
};

function calcVyse2(pocMes) { //Výpočet pro výslednou hodnotu v poli B106 „výše (Kč)“
	var r = "";
	if (
			getSum_jina_vyse_spl() != 0
			//getNumber (Zprava_pro_oddluzeni.jina_vyse_splatek.wrapper.navrh_na_stanoveni.zpusob_urceni.vyse) != 0 ||
			//getNumber (Zprava_pro_oddluzeni.jina_vyse_splatek.wrapper.navrh_na_stanoveni.zpusob_urceni.predpokladana_vyse)
		) 
		r = /*Math.ceil*/(
			//(getNumber (Zprava_pro_oddluzeni.jina_vyse_splatek.wrapper.navrh_na_stanoveni.zpusob_urceni.vyse) 
			//+ getNumber (Zprava_pro_oddluzeni.jina_vyse_splatek.wrapper.navrh_na_stanoveni.zpusob_urceni.predpokladana_vyse)//+ TODO pole vysse navrhovane splatky
			(getSum_jina_vyse_spl()
			- getOdm_ins_spr () ) * pocMes
			+ getNumber (Zprava_pro_oddluzeni.ocekavany_vytezek.wrapper.splatky_a_majetek_wrapper.splatky_a_majetek)
			- getOdm_prihl ()
			- getNumber (Zprava_pro_oddluzeni.vyzivovaci_povinnost.wrapper.dluznici.dluznik1.vyzivne_dluzne)
			- (getNumber (Zprava_pro_oddluzeni.vyzivovaci_povinnost.wrapper.dluznici.dluznik1.vyzivne.vyzivne_celkova_vyse) * pocMes)
			- getNumber (Zprava_pro_oddluzeni.vyzivovaci_povinnost.wrapper.dluznici.dluznik2.vyzivne_dluzne)
			- (getNumber (Zprava_pro_oddluzeni.vyzivovaci_povinnost.wrapper.dluznici.dluznik2.vyzivne.vyzivne_celkova_vyse) * pocMes)
			- calcDPH( getNumber (Zprava_pro_oddluzeni.ostatni.wrapper.odmena_sepsani.vyse_neuhrazeno), getNumber (Zprava_pro_oddluzeni.ostatni.wrapper.zpracovatel_platcem_dph_wrapper.zpracovatel_platcem_dph.zpracovatel_platcem_dph))
		);
	if (r &lt; 0) {
		r = 0;
	};		
	return r;
};

function calcMira2(pocMes) { 
	var r = 100 * calcVyse2(pocMes) / getNumber (Zprava_o_prezkumu.wrapper.celkem_bez_podrizenych_wrapper.celkem_bez_podrizenych);
	if (r &gt; 999.9999) {
