			+ getNumber (Zprava_pro_oddluzeni.vyzivovaci_povinnost.wrapper.dluznici.dluznik2.vyzivne_dluzne)
			- getNumber (Zprava_pro_oddluzeni.ocekavany_vytezek.wrapper.splatky_a_majetek_wrapper.splatky_a_majetek)
			+ calcDPH( getNumber (Zprava_pro_oddluzeni.ostatni.wrapper.odmena_sepsani.vyse_neuhrazeno), getNumber (Zprava_pro_oddluzeni.ostatni.wrapper.zpracovatel_platcem_dph_wrapper.zpracovatel_platcem_dph.zpracovatel_platcem_dph)))
			
			/
			( //getNumber (Zprava_pro_oddluzeni.jina_vyse_splatek.wrapper.navrh_na_stanoveni.zpusob_urceni.vyse) 
			//- getNumber (Zprava_pro_oddluzeni.jina_vyse_splatek.wrapper.navrh_na_stanoveni.zpusob_urceni.predpokladana_vyse)//- TODO pole vysse navrhovane splatky 
			getSum_jina_vyse_spl()
			- getOdm_ins_spr ()
			- getNumber (Zprava_pro_oddluzeni.vyzivovaci_povinnost.wrapper.dluznici.dluznik1.vyzivne.vyzivne_celkova_vyse)	
			- getNumber (Zprava_pro_oddluzeni.vyzivovaci_povinnost.wrapper.dluznici.dluznik2.vyzivne.vyzivne_celkova_vyse)	
			)
		);
	if (r &lt; 0 ) r = 0;
	return r;

};

function calcCelkBez() { //„Z celkové výše přezkoumaných pohledávek nezajištěných věřitelů bez podřízených pohledávek“
	var r = 100.0 * getNumber (Zprava_pro_oddluzeni.ocekavany_vytezek.wrapper.majetek_wrapper.majetek) / getNumber (Zprava_o_prezkumu.wrapper.celkem_bez_podrizenych_wrapper.celkem_bez_podrizenych);
