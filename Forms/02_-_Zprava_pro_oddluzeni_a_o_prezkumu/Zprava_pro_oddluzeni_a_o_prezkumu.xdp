function getNumber (obj) {
	try {
		if (obj == null) return 0;
		
		return (obj.rawValue==""||obj.rawValue==null?0:obj.rawValue*1);
	} catch (err_GN) {
		xfa.host.messageBox("Error: " + err_GN + "\nObj.name: " + obj.name);
