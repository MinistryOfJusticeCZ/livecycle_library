function  calcSrazka_dluznik (n) {
	var srazka = 0;
	var zabav_prijem = getSum_prijmy_podlehajici_dluznik(n) - getNezab_prijem_dluznik(n) ;
	if (zabav_prijem &lt;  getLimit_bezOmez()){
		srazka = 2*Math.floor(zabav_prijem/3);
	}
	else {
		srazka = 2*Math.floor(getLimit_bezOmez()/3) + (zabav_prijem - getLimit_bezOmez());
	};
	if (srazka &lt; 0) {
		srazka = 0;
	};	
	return srazka;
};
function  calcSplatka() {
	return calcSplatka_dluznik(0) + calcSplatka_dluznik(1);
};

function  calcSplatka_dluznik(n)  {
	var splatka = 0;
  //Souběh příjmů OSVČ a jiného příjmu
			// Zvolení možnosti „OSVČ“ v poli B57 a zároveň možnosti „OSVČ“ v poli B63
			if (existsOSVC_dluznik (n) &amp;&amp;  ( getCount_prijmy_podlehajici_dluznik (n) == 1 || (getZakladniCatka_dluznik(n)) == "1")) {
				splatka = getSum_podlehajici_OSVC_dluznik (n) +  getSum_prijmy_podlehajici_dluznik (n)  ;	
			}
			// Zvolení možnosti „OSVČ“ v poli B57 a zároveň možnosti „Jiný příjem dlužníka“ v poli B63
			if (existsOSVC_dluznik (n) &amp;&amp; (getZakladniCatka_dluznik(n)) == "2") {
				splatka = getSum_podlehajici_OSVC_dluznik (n) + calcSrazka_dluznik (n); 
			}
			if ( ! existsOSVC_dluznik (n) &amp;&amp; getCount_prijmy_podlehajici_dluznik (n) &gt; 0 ) {
				//Souběh příjmů podléhajících výkonu rozhodnutí s příjmy nepodléhajících výkonu rozhodnutí
				splatka = calcSrazka_dluznik (n);
			};
			
    splatka += getSum_prispevky(n);
      
    return splatka;
};
</script>
