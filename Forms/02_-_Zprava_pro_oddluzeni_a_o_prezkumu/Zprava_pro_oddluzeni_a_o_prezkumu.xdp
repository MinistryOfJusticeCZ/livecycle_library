function nez_na_pov() {
	return 3*(Zprava_pro_oddluzeni.mira_uspokojeni_veritelu_wrapper.mira_uspokojeni_veritelu.parametry.ziv_min.rawValue + Zprava_pro_oddluzeni.mira_uspokojeni_veritelu_wrapper.mira_uspokojeni_veritelu.parametry.Cell2.vyse_jina.rawValue)/4;
