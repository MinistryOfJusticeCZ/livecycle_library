         <script contentType="application/x-javascript" name="localLib">
function podpisConcatenate (objOsoba) {

	
	var podpis = "";
	if (objOsoba == null) 
		podpis = null
	else
		podpis = objOsoba.titul_pred.rawValue + objOsoba.jmeno.rawValue + objOsoba.prijmeni.rawValue + objOsoba.titul_za.rawValue;
	
	return podpis;
}


function print (oSekce_A, oSekce_B, oSekce_C, bPrePrint) {
	var hideShow = bPrePrint?"hidden":"visible";
	
	if (oSekce_A.rawValue === 0) {
		Zprava_o_prezkumu.presence = hideShow ;
		Pata.seznam_priloh.presence = hideShow ;
	}
	
	if (oSekce_B.rawValue === 0) {
		Zaznam_o_jednani.presence = hideShow ;
		Pata.seznam_priloh.presence = hideShow ;
	}

	if (oSekce_C.rawValue === 0) {
		Zprava_pro_oddluzeni.presence = hideShow ;
		Pata.seznam_priloh.presence = hideShow ;
	}
	
	Nastaveni_formulare.presence =  hideShow ;

	if ( bPrePrint ) {
		//§ oddíl A: „Zpráva o přezkumu“
			if (oSekce_A.rawValue === 1 &amp;&amp; oSekce_B.rawValue === 0 &amp;&amp; oSekce_C.rawValue === 0) sharedLib.setOnValueChange(ELZPPO.Hlavicka.title.title, "Zpráva o přezkumu")
		//§ oddíl B: „Záznam o jednání s dlužníkem“
			if (oSekce_A.rawValue === 0 &amp;&amp; oSekce_B.rawValue === 1 &amp;&amp; oSekce_C.rawValue === 0) sharedLib.setOnValueChange(ELZPPO.Hlavicka.title.title, "Záznam o jednání s dlužníkem")
		//§ oddíl C: „Zpráva pro oddlužení“
			if (oSekce_A.rawValue === 0 &amp;&amp; oSekce_B.rawValue === 0 &amp;&amp; oSekce_C.rawValue === 1) sharedLib.setOnValueChange(ELZPPO.Hlavicka.title.title, "Zpráva pro oddlužení")
		//§ oddíl A+B: „Zpráva o přezkumu“
			if (oSekce_A.rawValue === 1 &amp;&amp; oSekce_B.rawValue === 1 &amp;&amp; oSekce_C.rawValue === 0) sharedLib.setOnValueChange(ELZPPO.Hlavicka.title.title, "Zpráva o přezkumu")
		//§ oddíl B+C: „Zpráva pro oddlužení“
			if (oSekce_A.rawValue === 0 &amp;&amp; oSekce_B.rawValue === 1 &amp;&amp; oSekce_C.rawValue === 1) sharedLib.setOnValueChange(ELZPPO.Hlavicka.title.title, "Zpráva pro oddlužení")
		//§ oddíl A+C: „Zpráva pro oddlužení a o přezkumu“
			if (oSekce_A.rawValue === 1 &amp;&amp; oSekce_B.rawValue === 0 &amp;&amp; oSekce_C.rawValue === 1) sharedLib.setOnValueChange(ELZPPO.Hlavicka.title.title, "Zpráva pro oddlužení a o přezkumu")
		//§ oddíl A+B+C: „Zpráva pro oddlužení a o přezkumu“
			if (oSekce_A.rawValue === 1 &amp;&amp; oSekce_B.rawValue === 1 &amp;&amp; oSekce_C.rawValue === 1) sharedLib.setOnValueChange(ELZPPO.Hlavicka.title.title, "Zpráva pro oddlužení a o přezkumu")
		}
		else sharedLib.setOnValueChange(ELZPPO.Hlavicka.title.title, "Zpráva pro oddlužení a o přezkumu");
}

/*
	Zobrazí/Skryje část "Splátkový kalendář se zpeněžením majetkové podstaty při navrhované výši měsíční splátky" 
	v tabulce splátkového kalendáře v sekci C10 podle toho, jestli je u dlužníka nebo dlužnice podán Návrh na jinou výši splátek.
*/
function updateSplatkovyKalendar(jineVyseSplatek, splatkovyKalendar) {
	var count = jineVyseSplatek._jina_vyse_splatek.count;	
	
	for(i=0; i&lt;count; i++)
	{
    	if (jineVyseSplatek.resolveNode("jina_vyse_splatek[" + i + "]").wrapper.header.navrh_na_stanoveni.rawValue == "1")
    	{
    		sharedLib.visibleIfHidden(splatkovyKalendar.splatky_jine_header);
    		sharedLib.visibleIfHidden(splatkovyKalendar.splatky_jine);
    		sharedLib.visibleIfHidden(splatkovyKalendar.predpokladana_vyse_srazek.Cell2.vyse_jina);
    		
    		return;
    	}
	}
	
	sharedLib.hideIfVisible(splatkovyKalendar.splatky_jine_header);
	sharedLib.hideIfVisible(splatkovyKalendar.splatky_jine);	
	sharedLib.hideIfVisible(splatkovyKalendar.predpokladana_vyse_srazek.Cell2.vyse_jina);
}
