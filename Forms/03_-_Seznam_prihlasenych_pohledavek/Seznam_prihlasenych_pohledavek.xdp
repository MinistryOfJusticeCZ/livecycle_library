         <script contentType="application/x-javascript" name="localLib">
function generovana_priloha_setData(oPohledavka, oVeritel) {

	var sigField1 = event.target.getField("ELSEPP[0].podpis[0].wrapper[0].podpis_ins_spravce[0].podpisove_misto[0].podpis[0]");
	
	if (sigField1 != null &amp;&amp; sigField1.value != null &amp;&amp; sigField1.value.length&gt;0) {	
	      return;
    };

	
	var ID_Prilohy = null;
	if (oPohledavka != null) 
		ID_Prilohy = oPohledavka.id_prilohy.rawValue
	else 
		ID_Prilohy = oVeritel.id_prilohy.rawValue;
		
	if (ID_Prilohy == null || ID_Prilohy == "") return;
	
	
	oGenPriloha = getGenPril(ID_Prilohy);
	//xfa.host.messageBox("oGenPriloha " + oGenPriloha + "\noVeritel.propojeni_53.rawValue: " + oVeritel.propojeni_53.rawValue );	
	if (
			( oPohledavka != null
				&amp;&amp; (
					(oPohledavka.resolveNode("Cell8.popreno_por") != null &amp;&amp; 
						( oPohledavka.Cell8.popreno_por.rawValue &gt; 0 || oPohledavka.Cell8.popreno_vyse.rawValue &gt; 0 ) )
					//|| (oPohledavka.resolveNode("propojeni_53") != null &amp;&amp; oPohledavka.propojeni_53.rawValue == 1 )	
				)
			)		
			|| (  oPohledavka == null  
				&amp;&amp; (
					oVeritel.propojeni_53.rawValue == 1
					|| (oVeritel.resolveNode("navrh_hlas_prava") != null &amp;&amp; oVeritel.navrh_hlas_prava.rawValue == 1 )
					)
				)
		)
	{
			if (oGenPriloha == null) {
				var nPriloha = localLib.getPrezkumListNum_Nez(ID_Prilohy);
				var varStrana = 0;
				
				if (nPriloha != null) {
					varStrana = nPriloha;
				};

				nPriloha = localLib.getPrezkumListNum_Zaj(ID_Prilohy);
				if (nPriloha != null) {
					varStrana =_Priloha_nezajistena.count;
					varStrana += nPriloha ;
				};
				
				nPriloha =  localLib.getPrilohaNum_par53(ID_Prilohy);				
				if (nPriloha != null) {
					varStrana =_Priloha_nezajistena.count;
					varStrana +=_Priloha_zajistena.count;
					varStrana += nPriloha ;
				};

				nPriloha = localLib.getPrezkumListNum_Nezar(ID_Prilohy);
				if (nPriloha != null) {
					varStrana =_Priloha_nezajistena.count;
					varStrana +=_Priloha_zajistena.count;
					varStrana +=_Priloha_par_53.count;
					varStrana += nPriloha;
				};

				oGenPriloha = Seznam_priloh.wrapper.Generovane_prilohy.prilohy._priloha.insertInstance(varStrana, true);
				oGenPriloha.ident.id_prilohy.rawValue = ID_Prilohy;
			};
			if (oPohledavka != null) {
				sharedLib.setOnValueChange(oGenPriloha.c_pohl, oPohledavka.pohledavka.rawValue);
			}
			else {
				sharedLib.setOnValueChange(oGenPriloha.c_pohl, "");
				sharedLib.setOnFillColorChange(oGenPriloha.c_pohl, "225,225,225");
				//oGenPriloha.c_pohl.border.fill.color.value = "225,225,225";
//				oGenPriloha.c_pohl.fillColor = "5,5,5";
				//oGenPriloha.c_pohl.ui.fill.color.value  = "215,215,215";
			};
		
			sharedLib.setOnValueChange(oGenPriloha.veritel, oVeritel.veritel.rawValue);
			sharedLib.setOnValueChange(oGenPriloha.ident.c_prihl, oVeritel.prihlaska.rawValue);
	}
	else {
		if (oGenPriloha != null) oGenPriloha.parent._priloha.removeInstance(oGenPriloha.index);
	};
};

function priloha_par53_setData(oPrihlaska, setPar53, bNezajistene, bZajistene) {
	//var setPar53  = oPrihlaska.veritel.propojeni_53.rawValue;
	var sigField1 = event.target.getField("ELSEPP[0].podpis[0].wrapper[0].podpis_ins_spravce[0].podpisove_misto[0].podpis[0]");
	
	if (sigField1 != null &amp;&amp; sigField1.value != null &amp;&amp; sigField1.value.length&gt;0) {	      
		return;
    };

    var ID_Prilohy = oPrihlaska.veritel.id_prilohy.rawValue;
    var oPriloha = null;

    if (ID_Prilohy == null || ID_Prilohy == "" ) {
        if ( setPar53 == 1) {
        	var insInd = 0;
        	var limitNezajisteno = -1;
        	var limitZajisteno  = -1;
        	
        	if (bNezajistene) {
        		limitNezajisteno = oPrihlaska.parent.parent.index;
        	}
        	else {
        		if ( xfa.resolveNode("Pohledavky.zarazene.wrapper.nezajistene_wrapper.wrapper.wrapper_table") )
        			limitNezajisteno = 	Pohledavky.zarazene.wrapper.nezajistene_wrapper.wrapper.wrapper_table.nezajistene._row.count;
        		else
        			limitNezajisteno = 0;
        	};
        	
        	if (bZajistene) {
        		limitZajisteno = oPrihlaska.parent.index;
        	}
        	else {
        		limitZajisteno = 0;
        	};
        	
        	//Pohledavky.zarazene.wrapper.nezajistene_wrapper.wrapper.wrapper_table.nezajistene.row.pohledavky.rowSubSet.veritel.veritel_nazev
        	for (var i = 0; i &lt; limitNezajisteno ; i++) {
        		var oRow = xfa.resolveNode("Pohledavky.zarazene.wrapper.nezajistene_wrapper.wrapper.wrapper_table.nezajistene.row[" + i + "]");
        		if (oRow.pohledavky.rowSubSet.veritel.propojeni_53.rawValue == 1) insInd++;
        	};

//			xfa.host.messageBox(" limitNEZajisteno: " + limitNezajisteno + " insInd: " + insInd );        	
        	
        	for (var i = 0; i &lt; limitZajisteno ; i++) {
        		var oRow = xfa.resolveNode("Pohledavky.zarazene.wrapper.zajistene_wrapper.wrapper.wrapper_table.zajistene.row[" + i + "]");
        		if (oRow.pohledavky.veritel.propojeni_53.rawValue == 1) insInd++;
        	};

//			xfa.host.messageBox("limitZajisteno: " + limitZajisteno + " limitNEZajisteno: " + limitNezajisteno + " insInd: " + insInd );

            oPriloha = _Priloha_par_53.insertInstance(insInd, true);
            ID_Prilohy = localLib.genID();
            oPrihlaska.veritel.id_prilohy.rawValue = ID_Prilohy;
            oPriloha.ID = ID_Prilohy;
			generovana_priloha_setData(null, oPrihlaska.veritel);    
        }
        else return;
    }
    else {
        oPriloha = localLib.getPriloha_par53(ID_Prilohy);
        //xfa.host.messageBox("ID_Prilohy: " + ID_Prilohy + "\noPriloha: " + oPriloha );
        if ( setPar53 != 1 ) {
            oPriloha.parent._Priloha_par_53.removeInstance(oPriloha.index);
			generovana_priloha_setData(null, oPrihlaska.veritel);   
            oPrihlaska.veritel.id_prilohy.rawValue = null;
            ID_Prilohy = null;
            return;
        }
        else 
        	generovana_priloha_setData(null, oPrihlaska.veritel); 
    };
    
     if ( setPar53 == 1) {
        sharedLib.setOnValueChange(oPriloha.wrapper.hlavicka.veritel_nazev, oPrihlaska.veritel.veritel_nazev.rawValue);
		sharedLib.setOnValueChange(oPriloha.wrapper.hlavicka.prihlaska, oPrihlaska.veritel.prihlaska.rawValue);
     };
}

function getPrilohaNum_par53(cislo) {
	for (var i=0; i &lt;  _Priloha_par_53.count; i++) {
		oList = xfa.resolveNode("Priloha_par_53["+i+"]");
		if (oList.ID.rawValue == cislo) return i;
	}
}

function getPriloha_par53(cislo) {
	for (var i=0; i &lt;  _Priloha_par_53.count; i++) {
		oList = xfa.resolveNode("Priloha_par_53["+i+"]");
		if (oList.ID.rawValue == cislo) return oList;
	}
}

function priloha_setFocus(id_priloha) {
	var index = getPrezkumListNum_Nezar(id_priloha);
	//xfa.host.messageBox("ïndex " + index + " id: " + id_priloha);
	if (index != null) {
		oPriloha=xfa.resolveNode("ELSEPP.Priloha_nezarazena["+index+"]").wrapper;
		if (oPriloha._propojeni_53_wrapper.count &gt; 0)
			xfa.host.setFocus(oPriloha.propojeni_53_wrapper.propojeni_53)
		else
			xfa.host.setFocus(oPriloha.navrh_hlas_prav_wrapper.v_rozsahu)
	};
	
	index =  getPrilohaNum_par53(id_priloha);
	if (index != null) {
		oPriloha=xfa.resolveNode("ELSEPP.Priloha_par_53["+index+"]").wrapper;
		xfa.host.setFocus(oPriloha.propojeni_53);
	};
	
	index = getPrezkumListNum_Nez(id_priloha);
	if (index != null) {
		oPriloha=xfa.resolveNode("ELSEPP.Priloha_nezajistena["+index+"]").wrapper;
		xfa.host.setFocus(oPriloha.wrapper.celkova_vyse);
	};

	index = getPrezkumListNum_Zaj(id_priloha);
	if (index != null) {
		oPriloha=xfa.resolveNode("ELSEPP.Priloha_zajistena["+index+"]").wrapper;
		xfa.host.setFocus(oPriloha.wrapper.celkova_vyse);
	};	
}


function priloha_nezarazena_setData(oVeritel) {
	var sigField1 = event.target.getField("ELSEPP[0].podpis[0].wrapper[0].podpis_ins_spravce[0].podpisove_misto[0].podpis[0]");
	
	if (sigField1 != null &amp;&amp; sigField1.value != null &amp;&amp; sigField1.value.length&gt;0) {
		return;
    };
    
    var setHlasPravo= oVeritel.navrh_hlas_prava.rawValue;
    var setPar53 = oVeritel.propojeni_53.rawValue;
    var ID_Prilohy = oVeritel.id_prilohy.rawValue;
    
    var oPriloha = null;

	generovana_priloha_setData(null, oVeritel);    
    
    if (ID_Prilohy == null || ID_Prilohy == "") {
        if (setHlasPravo == 1 || setPar53 == 1) {
        	oPriloha = _Priloha_nezarazena.insertInstance(oVeritel.parent.parent.index &gt; _Priloha_nezarazena.count?_Priloha_nezarazena.count:oVeritel.parent.parent.index, true ) ;
            ID_Prilohy = localLib.genID();
            oVeritel.id_prilohy.rawValue = ID_Prilohy;
            oPriloha.ID = ID_Prilohy;
			generovana_priloha_setData(null, oVeritel);    
        }
        else return;
    }
    else {
        oPriloha = localLib.getPrezkumList_Nezar(ID_Prilohy)
        if (setHlasPravo != 1 &amp;&amp; setPar53 != 1) {
            oPriloha.parent._Priloha_nezarazena.removeInstance(oPriloha.index);
			generovana_priloha_setData(null, oVeritel);   
            oVeritel.id_prilohy.rawValue = null;
            ID_Prilohy = null;
            return;
        }
    };
    
     if (setHlasPravo == 1 || setPar53 == 1) {
        sharedLib.setOnValueChange(oPriloha.wrapper.hlavicka.veritel_wrapper.veritel_nazev, oVeritel.veritel_nazev.rawValue);
		sharedLib.setOnValueChange(oPriloha.wrapper.hlavicka.prihlaska_wrapper.prihlaska, oVeritel.prihlaska.rawValue);
     };

    if (setHlasPravo == 1) {
        if (oPriloha.wrapper._navrh_hlas_prav_wrapper.count == 0)  oPriloha.wrapper._navrh_hlas_prav_wrapper.addInstance(1);
	}
    else  
        if (oPriloha.wrapper._navrh_hlas_prav_wrapper.count != 0)  oPriloha.wrapper._navrh_hlas_prav_wrapper.setInstances(0);
    
    if ( setPar53 == 1) {
        if (oPriloha.wrapper._propojeni_53_wrapper.count == 0)  oPriloha.wrapper._propojeni_53_wrapper.addInstance(1);
    }
    else 
        if (oPriloha.wrapper._propojeni_53_wrapper.count != 0)  oPriloha.wrapper._propojeni_53_wrapper.setInstances(0);
}


function priloha_zajistena_setData(oPohledavka) {
	var sigField1 = event.target.getField("ELSEPP[0].podpis[0].wrapper[0].podpis_ins_spravce[0].podpisove_misto[0].podpis[0]");
	
	if (sigField1 != null &amp;&amp; sigField1.value != null &amp;&amp; sigField1.value.length&gt;0) {
		return;
    };
    
    var setPrezkum = 0;
    if (oPohledavka.castky.Cell8.popreno_por.rawValue &gt; 0 || oPohledavka.castky.Cell8.popreno_vyse.rawValue &gt; 0 )
    	setPrezkum = 1;
    var ID_Prilohy = oPohledavka.castky.id_prilohy.rawValue;
    
    var oPriloha = null;

	generovana_priloha_setData(oPohledavka.castky, oPohledavka.parent.veritel);    
    
    if (ID_Prilohy == null || ID_Prilohy == "") {
        if (setPrezkum == 1 ) {
        	var insIndex=0;
        	for (var i=0;  i &lt; oPohledavka.parent.parent.index ; i++) {
//        		xfa.host.messageBox ("1. VerInd: " + i + " insIndex: " + insIndex);
        		oRow = xfa.resolveNode("Pohledavky.zarazene.wrapper.zajistene_wrapper.wrapper.wrapper_table.zajistene.row[" + i + "]");
        		//Pohledavky.zarazene.wrapper.zajistene_wrapper.wrapper.wrapper_table.zajistene.row.pohledavky.veritel.veritel_nazev
        		//Pohledavky.zarazene.wrapper.zajistene_wrapper.wrapper.wrapper_table.zajistene.row.pohledavky.pohledavka.castky.pravni_duvod
        		//Pohledavky.zarazene.wrapper.zajistene_wrapper.wrapper.wrapper_table.zajistene.row.pohledavky.pohledavka.castky.popirajici
        		for (var j=0;  j &lt; oRow.pohledavky._pohledavka.count; j++) {
//        			xfa.host.messageBox ("2. VerInd: " + i + " PohlInd: " + j + " insIndex: " + insIndex);
        			oPohl = oRow.resolveNode("pohledavky.pohledavka[" + j + "]");
        			if (oPohl.castky.Cell8.popreno_por.rawValue &gt; 0 || oPohl.castky.Cell8.popreno_vyse.rawValue &gt; 0) {
        				insIndex++ ;
        			};
        		};
        	};
 
//       	xfa.host.messageBox ("posledni veritel");
        	for (var j=0;  j &lt; oPohledavka.index; j++) {
//        		xfa.host.messageBox ("VerInd: LAST PohlInd: " + j + " insIndex: " + insIndex);
        		var oPohl = oPohledavka.parent.resolveNode("pohledavka[" + j + "]");
        		if (oPohl.castky.Cell8.popreno_por.rawValue &gt; 0 || oPohl.castky.Cell8.popreno_vyse.rawValue &gt; 0 ) insIndex++ ;
        	};
//        	xfa.host.messageBox ("insIndex: " + insIndex);
            oPriloha = _Priloha_zajistena.insertInstance(insIndex, true);
        
        
            ID_Prilohy = localLib.genID();
            oPohledavka.castky.id_prilohy.rawValue = ID_Prilohy;
            oPriloha.ID = ID_Prilohy;
			generovana_priloha_setData(oPohledavka.castky, oPohledavka.parent.veritel);    
        }
        else return;
    }
    else {
        oPriloha = localLib.getPrezkumList_Zaj(ID_Prilohy)
        if (setPrezkum != 1 ) {
            oPriloha.parent._Priloha_zajistena.removeInstance(oPriloha.index);
			generovana_priloha_setData(oPohledavka.castky, oPohledavka.parent.veritel);   
            oPohledavka.castky.id_prilohy.rawValue = null;
            ID_Prilohy = null;
            return;
