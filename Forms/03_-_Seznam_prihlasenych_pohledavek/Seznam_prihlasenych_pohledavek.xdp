function compareFields(field1, field2) {
	var field1Value = field1.rawValue != null ? field1.rawValue : 0;
	var field2Value = field2.rawValue != null ? field2.rawValue : 0;
	
	return field1Value == field2Value ? 0 : field1Value &gt; field2Value ? 1 : -1;
}

