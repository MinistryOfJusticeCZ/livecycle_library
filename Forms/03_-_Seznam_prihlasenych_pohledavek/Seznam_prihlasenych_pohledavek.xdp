function priloha_nezajistena_setData(oPohledavka) {
	var sigField1 = event.target.getField("ELSEPP[0].podpis[0].wrapper[0].podpis_ins_spravce[0].podpisove_misto[0].podpis[0]");
	
	if (sigField1 != null &amp;&amp; sigField1.value != null &amp;&amp; sigField1.value.length&gt;0) {
		return;
    };
    
    var setPrezkum = 0;
    if (oPohledavka.Cell8.popreno_por.rawValue &gt; 0 || oPohledavka.Cell8.popreno_vyse.rawValue &gt; 0 )
    	setPrezkum = 1;
    var ID_Prilohy = oPohledavka.id_prilohy.rawValue;
    
    var oPriloha = null;

	generovana_priloha_setData(oPohledavka, oPohledavka.parent.veritel);    
    
    if (ID_Prilohy == null || ID_Prilohy == "") {
        if (setPrezkum == 1) {
        	var insIndex=0;
        	for (var i=0;  i &lt; oPohledavka.parent.parent.parent.index ; i++) {
        		//Pohledavky.zarazene.wrapper.nezajistene_wrapper.wrapper.wrapper_table.nezajistene.row.pohledavky.rowSubSet.veritel.veritel_nazev
        		//Pohledavky.zarazene.wrapper.nezajistene_wrapper.wrapper.wrapper_table.nezajistene.row.pohledavky.rowSubSet.pohledavka.pravni_duvod
//        		xfa.host.messageBox ("1. VerInd: " + i + " insIndex: " + insIndex);
        		oRow = xfa.resolveNode("Pohledavky.zarazene.wrapper.nezajistene_wrapper.wrapper.wrapper_table.nezajistene.row[" + i + "]");
        		for (var j=0;  j &lt; oRow.pohledavky.rowSubSet._pohledavka.count; j++) {
//        			xfa.host.messageBox ("2. VerInd: " + i + " PohlInd: " + j + " insIndex: " + insIndex);
        			oPohl = oRow.resolveNode("pohledavky.rowSubSet.pohledavka[" + j + "]");
        			if (oPohl.Cell8.popreno_por.rawValue &gt; 0 || oPohl.Cell8.popreno_vyse.rawValue &gt; 0) {
        				insIndex++ ;
        			};
        		};
        	};
 
//	       	xfa.host.messageBox ("posledni veritel");
        	for (var j=0;  j &lt; oPohledavka.index; j++) {
//        		xfa.host.messageBox ("VerInd: LAST PohlInd: " + j + " insIndex: " + insIndex);
        		var oPohl = oPohledavka.parent.resolveNode("pohledavka[" + j + "]");
        		if (oPohl.Cell8.popreno_por.rawValue &gt; 0 || oPohl.Cell8.popreno_vyse.rawValue &gt; 0) insIndex++ ;
        	};
//        	xfa.host.messageBox ("insIndex: " + insIndex);
            oPriloha = _Priloha_nezajistena.insertInstance(insIndex, true);

            ID_Prilohy = localLib.genID();
            oPohledavka.id_prilohy.rawValue = ID_Prilohy;
            oPriloha.ID = ID_Prilohy;
			generovana_priloha_setData(oPohledavka, oPohledavka.parent.veritel);    
        }
        else return;
    }
    else {
        oPriloha = localLib.getPrezkumList_Nez(ID_Prilohy)
        if (setPrezkum != 1) {
            oPriloha.parent._Priloha_nezajistena.removeInstance(oPriloha.index);
			generovana_priloha_setData(oPohledavka, oPohledavka.parent.veritel);   
            oPohledavka.id_prilohy.rawValue = null;
            ID_Prilohy = null;
            return;
        }
    };
              
   	sharedLib.setOnValueChange(oPriloha.wrapper.wrapper.celkova_vyse, oPohledavka.prihlaseno.rawValue);
	sharedLib.setOnValueChange(oPriloha.wrapper.vykonatelna.vykonatelna_vyse, oPohledavka.vykonatelne.rawValue);
	sharedLib.setOnValueChange(oPriloha.wrapper.podrizena_vyse, oPohledavka.nepodrizeno.rawValue);	
	sharedLib.setOnValueChange(oPriloha.wrapper.pravni_duvod, oPohledavka.pravni_duvod.rawValue);
	
	sharedLib.setOnValueChange(oPriloha.wrapper.duplicitni_wrapper.pohledavka, oPohledavka.duplicitni.rawValue);
	if (oPohledavka.duplicitni.rawValue != null &amp;&amp; oPohledavka.duplicitni.rawValue != "")
		sharedLib.visibleIfHidden(oPriloha.wrapper.duplicitni_wrapper.vyse);
	else
		sharedLib.hideIfVisible(oPriloha.wrapper.duplicitni_wrapper.vyse);
		
	sharedLib.setOnValueChange(oPriloha.wrapper.podminena_wrapper.druh_podminky, oPohledavka.druh_podminky.rawValue);
	sharedLib.setOnValueChange(oPriloha.wrapper.podminena_wrapper.podminena, oPohledavka.druh_podminky.rawValue != null &amp;&amp; oPohledavka.druh_podminky.rawValue != "" ? "1" : "0");

	sharedLib.setOnValueChange(oPriloha.wrapper.hlavicka.veritel_nazev, oPohledavka.parent.veritel.veritel_nazev.rawValue);
	
	sharedLib.setOnValueChange(oPriloha.wrapper.hlavicka.prihlaska, oPohledavka.parent.veritel.prihlaska.rawValue);
	sharedLib.setOnValueChange(oPriloha.wrapper.hlavicka.pohledavka, oPohledavka.pohledavka.rawValue);
}


