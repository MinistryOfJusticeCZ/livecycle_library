function updateFromDatumUsneseni(oDtUpadku, bExit) {
	// 1.. Očekávaná míra uspokojení nezajištěných věřitelů
	// 2.. Očekávaná míra uspokojení nezajištěných věřitelů z nepodřízených částí pohledávek
	
	//util.scand("mm/dd/yyyy", getField("DiscountEnds").value);
	
	
	var bPred = null;
	
	if (oDtUpadku.rawValue != null &amp;&amp; oDtUpadku.rawValue != "" /*&amp;&amp; oDtUpadku.rawValue != oDtUpadku.formattedValue*/) {
		
		bPred  = (sharedLib.compareDates(oDtUpadku, "2019-05-31") != 1);
	};
	
	/*xfa.host.messageBox("pred: " + bPred +
	" oDtUpadku.rawValue: " + oDtUpadku.rawValue + 
	" oDtUpadku.value: " + oDtUpadku.value + 
	""//" util.scand: " + util.scand("dd.mm.yyyy", oDtUpadku.formattedValue)
	 );
	*/
	
	if (bPred) {
		if (_Zopo.count &gt; 0) {
			sharedLib.visibleIfHidden(Zopo.zprava_spravce.wrapper.mira_uspokojeni.porizene);
			sharedLib.hideIfVisible(Zopo.zprava_spravce.wrapper.mira_uspokojeni.neporizene);
		};
			
		sharedLib.visibleIfHidden(ELZOPO.celkem_IS.porizene);
		sharedLib.hideIfVisible(ELZOPO.celkem_IS.neporizene);
		if (bExit) {
			celkem_IS.neporizene.Cell7.celkem_ocek_uspokojeni_3.rawValue = null;
			celkem_IS.neporizene.Cell7.celkem_ocek_uspokojeni_5.rawValue = null;
		};
		sharedLib.hideIfVisible(ELZOPO.celkem_IS.neporizene);
	}
	if (! bPred) {
		if (_Zopo.count &gt; 0) {
			sharedLib.visibleIfHidden(Zopo.zprava_spravce.wrapper.mira_uspokojeni.neporizene);
			sharedLib.hideIfVisible(Zopo.zprava_spravce.wrapper.mira_uspokojeni.porizene);
		};
			
		sharedLib.visibleIfHidden(ELZOPO.celkem_IS.neporizene);
