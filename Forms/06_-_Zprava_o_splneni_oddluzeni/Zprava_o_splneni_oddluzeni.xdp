/*
	Zvaliduje hodnotu pole zalohove_neuhrazeno. 
	Pokud je hodnota vyssi, nez u referencniho pole, zobrazi hlasku a vynuti focus.
*/
function validateZalohoveNeuhrazeno(zalohove_neuhrazeno, vyse) {
	if (zalohove_neuhrazeno.rawValue &gt; vyse.rawValue) {
		xfa.host.messageBox("Neuhrazená část nesmí být vyšší, než stanovená odměna.", "Chybná hodnota pole");
		xfa.host.setFocus(zalohove_neuhrazeno);
	};
}

/*
	Zvaliduje hodnotu pole uspokojit zbyva. 
	Pokud je hodnota vyssi, nez u referencniho pole, zobrazi hlasku a vynuti focus.
*/
function validateZbyva(zbyva, vyse) {
	if (zbyva.rawValue &gt; vyse.rawValue) {
		xfa.host.messageBox("Částka uspokojit zbývá nesmí být vyšší, než stanovená odměna.", "Chybná hodnota pole");
		xfa.host.setFocus(zbyva);
	};
}

