         <script contentType="application/x-javascript" name="localLib">

var disableValPrisl = false;
var dependCheckValPrisl = false;

function precislujPrisl(oRoot, ind, delta) {

//	xfa.host.messageBox("index: " + ind + " delta: " + delta);
	for (var o = ind; o &lt; oRoot._pohledavka.count; o++) {
	
		var oPohl = oRoot.resolveNode("Formular.pohledavky.pohledavka[" + o + "]");
	
		setAllPohledavkaPrisl (oPohl.resolveNode("wrapper.nezajistena_hromadne.prislusenstvi_wrapper.prislusenstvi"));	
		
		if ( oPohl.resolveNode("wrapper.nezajistena_hromadne.prislusenstvi_wrapper.prislusenstvi.tbl_prislusenstvi.prislusenstvi_row") ) {
			for (var i = oPohl.wrapper.nezajistena_hromadne.prislusenstvi_wrapper.prislusenstvi.tbl_prislusenstvi._prislusenstvi_row.count-1; i &gt;= 0 ; i--) {
				var oRow = oPohl.resolveNode("wrapper.nezajistena_hromadne.prislusenstvi_wrapper.prislusenstvi.tbl_prislusenstvi.prislusenstvi_row["+i+"]");
				//xfa.host.messageBox("delta: " + delta + "o: " + o + "i: " + i + " oRow: " + oRow);
				oRow.prislusenstvi_row_wrapper.pohledavka.rawValue = delta * 1 + oRow.prislusenstvi_row_wrapper.pohledavka.rawValue * 1;
			};
		
		};
	}

}


function setAllPohledavkaPrisl (objPrislusenstvi) {

if (objPrislusenstvi == null) return;

for (var i = 0; i &lt; objPrislusenstvi.tbl_prislusenstvi._prislusenstvi_row.count; i++) {
	setPohledavkaPrisl (objPrislusenstvi.resolveNode("tbl_prislusenstvi.prislusenstvi_row["+i+"].prislusenstvi_row_wrapper.pohledavka"));
}
	
}

function setPohledavkaPrisl (objPohledavka) {
		
		var oPohledavky = objPohledavka.parent.parent.parent.parent.parent.parent.pohledavky
		
		objPohledavka.clearItems();
		
		for (var i = 0; i &lt; oPohledavky._pohledavka_row.count; i++) {
			var txtItem = "" + oPohledavky.resolveNode("pohledavka_row["+i+"].poradi").rawValue ;
			objPohledavka.addItem(txtItem);
			//xfa.host.messageBox("i " + i + " txt " + txtItem)
		};
};

function getPohledavkaNum(id, parentObj) {
	for (var i=0; i &lt;  parentObj._pohledavka_row.count; i++) {
		oList =  parentObj.resolveNode("pohledavka_row["+i+"]");
		if (oList.buttons.id_pohledavky.rawValue == id) return i;
	}
};

function getFirstPrislNum(id, parentObj) {
	for (var i=0; i &lt;  parentObj.prislusenstvi_wrapper.prislusenstvi.tbl_prislusenstvi._prislusenstvi_row.count; i++) {
		oList = parentObj.resolveNode("prislusenstvi_wrapper.prislusenstvi.tbl_prislusenstvi.prislusenstvi_row["+i+"]");
		//xfa.host.messageBox("i" + oList.id_pohledavky.rawValue);
		if (oList.prislusenstvi_row_wrapper.pohledavka.rawValue == id) return i;
	}
}

function getSplatnostNum(id, parentObj) {
	for (var i=0; i &lt;  parentObj.vlastnosti.splatna._splatna_info.count; i++) {
		oList = parentObj.resolveNode("vlastnosti.splatna.splatna_info["+i+"]");
		//xfa.host.messageBox("i" + oList.id_pohledavky.rawValue);
		if (oList.id_pohledavky.rawValue == id) return i;
	}
}

function getPvjNum(id, parentObj) {
	for (var i=0; i &lt;  parentObj.vlastnosti.penezita._penezita_info.count; i++) {
		oList = parentObj.resolveNode("vlastnosti.penezita.penezita_info["+i+"]");
		//xfa.host.messageBox("i" + oList.id_pohledavky.rawValue);
		if (oList.id_pohledavky.rawValue == id) return i;
	}
}

function getVykonatelnostNum(id, parentObj) {
	for (var i=0; i &lt;  parentObj.vykonatelnost_wrapper._vykonatelnost.count; i++) {
		oList =  parentObj.resolveNode("vykonatelnost_wrapper.vykonatelnost["+i+"]");
		if (oList.wrapper.id_vykonatelnost.rawValue == id) return i;
	}
}

function genID() {
	var ID = "ISIR_" + String(new Date().getTime());
	return ID;
};

function podpisConcatenate (objOsoba) {

	
	var podpis = "";
	if (objOsoba == null) 
		podpis = null
	else
		podpis = objOsoba.titul_pred.rawValue + objOsoba.jmeno.rawValue + objOsoba.prijmeni.rawValue + objOsoba.titul_za.rawValue;
	
	return podpis;
}

function getVysePrisl(oPrislusenstvi) {

	var ret = 0;
	if (
			oPrislusenstvi.rb_prislusenstvi.rawValue == 1 &amp;&amp; 
			oPrislusenstvi.resolveNode("prislusenstvi.tbl_prislusenstvi.prislusenstvi_row[0].prislusenstvi_row_wrapper.vyse") != null
		) {
			
			for (var i = 0; i &lt; oPrislusenstvi.resolveNode("prislusenstvi.tbl_prislusenstvi")._prislusenstvi_row.count; i++ ) {
				ret += oPrislusenstvi.resolveNode("prislusenstvi.tbl_prislusenstvi.prislusenstvi_row["+i+"].prislusenstvi_row_wrapper.vyse").rawValue;
			};
		};	
	return ret;
};
