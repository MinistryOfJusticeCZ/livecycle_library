function PreSignCheck_local() {

	if (Formular.podpis.wrapper.podpisy.switch.rawValue == "1") {
		Formular.podpis.wrapper.podpisy.epodatelna.guid.rawValue = uuid_generator.createUUID();
	}
	else Formular.podpis.wrapper.podpisy.epodatelna.guid.rawValue = null;
//	if (xfa.event.cancelAction != 1) sharedLib.visibleIfHidden(Formular.podpis.wrapper.podpisy.podpis1.podpisove_misto.wrapper);
	if (sharedLib.excMaxAttachSize(event.target)) {
	  app.alert("Je překročena maximální povolená celková velikost příloh "+sharedLib.MAX_ATT_DOCS_SIZE+" MB!");
	  xfa.event.cancelAction = 1;
	}

	if (xfa.event.cancelAction != 1) 	Formular.QR.QR_lock.rawValue = 1
	
	
	if (xfa.event.cancelAction != 1) {
		var r = new RegExp(); 
		var message = "";
	
		r.compile ("^[2-9a-km-np-z]{7}$");
	
		var oIDDS = xfa.resolveNode("Formular.dluznik.wrapper.fyzicka_osoba.jiny_kontakt_wrapper.jiny_kontakt_veritel.idds");	
		if( oIDDS != null &amp;&amp; r.test(oIDDS.rawValue) == "0" &amp;&amp; oIDDS.rawValue != null &amp;&amp; oIDDS.rawValue != "") {
			message += "Chybně zadaný údaj Datová schránka dlužníka.\n";
			
		};
		oIDDS = xfa.resolveNode("Formular.dluznik.wrapper.pravnicka_osoba.jiny_kontakt_wrapper.jiny_kontakt_veritel.idds");
		if( oIDDS != null &amp;&amp; r.test(oIDDS.rawValue) == "0" &amp;&amp; oIDDS.rawValue != null &amp;&amp; oIDDS.rawValue != "") {
			message += "Chybně zadaný údaj Datová schránka dlužníka.\n";
			
		};
		
		for (var ver = 0; ver &lt; Formular.veritel.wrapper._veritel_wrapper.count; ver++) {
			oIDDS = xfa.resolveNode("Formular.veritel.wrapper.veritel_wrapper["+ver+"].veritel.fyzicka_osoba.jiny_kontakt_wrapper_veritel.jiny_kontakt_veritel.idds");
			if( oIDDS != null &amp;&amp; r.test(oIDDS.rawValue) == "0" &amp;&amp; oIDDS.rawValue != null &amp;&amp; oIDDS.rawValue != "") {
				message += "Chybně zadaný údaj Datová schránka věřitele č: " + (ver + 1) + ".\n";
				
			};
			
			oIDDS = xfa.resolveNode("Formular.veritel.wrapper.veritel_wrapper["+ver+"].veritel.pravnicka_osoba.jiny_kontakt_wrapper_veritel.jiny_kontakt_veritel.idds");
			if( oIDDS != null &amp;&amp; r.test(oIDDS.rawValue) == "0" &amp;&amp; oIDDS.rawValue != null &amp;&amp; oIDDS.rawValue != "") {
				message += "Chybně zadaný údaj Datová schránka věřitele č: " + (ver + 1) + ".\n";
				
			};
			
			oIDDS = xfa.resolveNode("Formular.veritel.wrapper.veritel_wrapper["+ver+"].zastupce_wrapper.zastupce.fyzicka_osoba.jiny_kontakt_wrapper_veritel.jiny_kontakt_veritel.idds");
			if( oIDDS != null &amp;&amp; r.test(oIDDS.rawValue) == "0" &amp;&amp; oIDDS.rawValue != null &amp;&amp; oIDDS.rawValue != "") {
				message += "Chybně zadaný údaj Datová schránka zástupce věřitele č: " + (ver + 1) + ".\n";
				
			};
			
					
			oIDDS = xfa.resolveNode("Formular.veritel.wrapper.veritel_wrapper["+ver+"].zastupce_wrapper.zastupce.pravnicka_osoba.jiny_kontakt_wrapper_veritel.jiny_kontakt_veritel.idds");
			if( oIDDS != null &amp;&amp; r.test(oIDDS.rawValue) == "0" &amp;&amp; oIDDS.rawValue != null &amp;&amp; oIDDS.rawValue != "") {
				message += "Chybně zadaný údaj Datová schránka zástupce věřitele č: " + (ver + 1) + ".\n";
				
			};
		}
		
		if (message != "") {
			xfa.host.messageBox(message);
			xfa.event.cancelAction = 1;
		};
	}
	
	if (xfa.event.cancelAction != 1) {
		var message = "";
		for (var pohl = 0; pohl &lt; Formular.pohledavky._pohledavka.count; pohl++) {
			var oPohl = xfa.resolveNode("Formular.pohledavky.pohledavka["+pohl+"].wrapper.nezajistena_hromadne")
			if (oPohl  != null) {
