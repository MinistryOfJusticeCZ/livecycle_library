	}
	else {
		if (localLib.getFirstPrislNum(poradi.rawValue, this.parent.parent.parent) == null) {
			var prisl = prislusenstvi_wrapper.prislusenstvi.tbl_prislusenstvi._prislusenstvi_row.addInstance(1);
			prisl.prislusenstvi_row_wrapper.pohledavka.rawValue = poradi.rawValue;
			prisl.prislusenstvi_row_wrapper.vyse.rawValue = this.rawValue;
			
			localLib.setPohledavkaPrisl (prisl.prislusenstvi_row_wrapper.pohledavka);
		};
	};

    
	localLib.dependCheckValPrisl = true;
	this.execValidate();
	localLib.dependCheckValPrisl = false;
}
else {
	if (xfa.resolveNode("prislusenstvi_wrapper.prislusenstvi")) {
		var i = localLib.getFirstPrislNum(poradi.rawValue, this.parent.parent.parent);
		//xfa.host.messageBox("1.. i " + i);
		
		while (i != null ) {
			if ( prislusenstvi_wrapper.prislusenstvi.tbl_prislusenstvi._prislusenstvi_row.count == 1) {
				prislusenstvi_wrapper.rb_prislusenstvi.rawValue = 0;
				prislusenstvi_wrapper.rb_prislusenstvi.execEvent("change");
				break;
			}
			else prislusenstvi_wrapper.prislusenstvi.tbl_prislusenstvi._prislusenstvi_row.removeInstance(i);
			
						
			i = localLib.getFirstPrislNum(poradi.rawValue, this.parent.parent.parent);
