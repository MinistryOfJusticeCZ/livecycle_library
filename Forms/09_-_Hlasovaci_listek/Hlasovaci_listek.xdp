         <script contentType="application/x-javascript" name="localLib">
function dluznikPodnikatel(dluznik) {
	if (dluznik.wrapper._fyzicka_osoba.count &gt; 0) {
		if (dluznik.wrapper.resolveNode("fyzicka_osoba[0].udaje_osoba_wrapper.udaje_osoba.ico").rawValue != null) {
			return true;
		}
		
		if (dluznik.wrapper._fyzicka_osoba.count &gt; 1 &amp;&amp; dluznik.wrapper.resolveNode("fyzicka_osoba[1].udaje_osoba_wrapper.udaje_osoba.ico").rawValue != null) {
			return true;
		}
	} else if (dluznik.wrapper._pravnicka_osoba.count &gt; 0) {
		return true;
	}
	return false;
}
</script>
