}
else if(this.rawValue == 346){
	sharedLib.setOnValueChange( ELHLAL.Formular.title.title,"Hlasovací lístek - o reorganizačním plánu");
	ELHLAL.Formular._hlasovani50.setInstances(0);
	ELHLAL.Formular._hlasovani401.setInstances(0);
	ELHLAL.Formular._hlasovani346.setInstances(1);
	sharedLib.setOnValueChange(ELHLAL.Formular.podpis.wrapper.label,"VII. Podpis");
	// spolecne oddluzeni neni mozne
	if(dluznik.wrapper.dluznik_wrapper.spolecne_oddluzeni.rawValue===1) {
		dluznik.wrapper.dluznik_wrapper.spolecne_oddluzeni.rawValue=0;
		dluznik.wrapper.dluznik_wrapper.spolecne_oddluzeni.execEvent("change");
	}
	sharedLib.hideIfVisible(dluznik.wrapper.dluznik_wrapper.spolecne_oddluzeni, 0);
}
else if(this.rawValue == 401){
	sharedLib.setOnValueChange( ELHLAL.Formular.title.title,"Hlasovací lístek - způsob oddlužení");
	ELHLAL.Formular._hlasovani50.setInstances(0);
	ELHLAL.Formular._hlasovani346.setInstances(0);
	ELHLAL.Formular._hlasovani401.setInstances(1);	
