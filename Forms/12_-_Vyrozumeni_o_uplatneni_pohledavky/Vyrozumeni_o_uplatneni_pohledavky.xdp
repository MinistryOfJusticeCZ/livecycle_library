                     <script contentType="application/x-javascript" runAt="client">
if (Formular.insolvencni_spravce.wrapper.druh_osoby.druh.rawValue != null || Formular.osoba_s_disp_opr.wrapper.druh_osoby.druh.rawValue != null) {    
    if (Formular.insolvencni_spravce.wrapper.druh_osoby.druh.rawValue == "3" || Formular.insolvencni_spravce.wrapper.druh_osoby.druh.rawValue == null)
        fillAdresat(Formular.osoba_s_disp_opr.wrapper);        
    else
        fillAdresat(Formular.insolvencni_spravce.wrapper);
}

function fillAdresat(field) {    
    if (field.druh_osoby.druh.rawValue == "1") {
        var subjekt = field.resolveNode("fyzicka_osoba[0]");

        if (subjekt == null)
            return;

        this.lib.setAdresat(subjekt.udaje_osoba_wrapper.udaje_osoba, null, null, subjekt.adresa_fo_bez_korespondencni_wrapper.adresa);

    } else if (field.druh_osoby.druh.rawValue == "2") {
        var subjekt = field.resolveNode("pravnicka_osoba[0]");

        if (subjekt == null)
            return;

        this.lib.setAdresat(null, subjekt.udaje_firma_wrapper.udaje_firma, null, subjekt.adresa_po_bez_korespondencni_wrapper.adresa);
    }
}
</script>
