         <script contentType="application/x-javascript" name="cz_naceLib">
/*
	Aktualizuje polozky drop-down "oblast" sekce CZ_NACE podle vybrane hodnoty drop-down "sekce".
	Hodnota drop-down "sekce" se uklada do skryteho pole "aktualni". 
	Pokud je v poli "aktualni" a hodnota dropdown "sekce" stejná, polozky se neaktualizuji.
*/
function update(cz_nace) {

	var sekce = cz_nace.wrapper.content.sekce.rawValue;
	var oblast = cz_nace.wrapper.content.oblast.rawValue;
	var nastavitOblast = false;
	
	if (sekce == null) {
		sharedLib.hideIfVisible(cz_nace.wrapper.content.oblast);
	} else {
		sharedLib.visibleIfHidden(cz_nace.wrapper.content.oblast);
	}
	
	if (cz_nace.wrapper.content.sekceAktualni.rawValue == sekce)
		return;

/*		
	if (oblast == null) {
		sharedLib.hideIfVisible(cz_nace.wrapper.content.oblast);
		return;
	} else {
		sharedLib.visibleIfHidden(cz_nace.wrapper.content.oblast);
	}
*/
	cz_nace.wrapper.content.oblast.rawValue = null;
	cz_nace.wrapper.content.oblast.clearItems();
	
	for (key in hodnoty[sekce]) {
		cz_nace.wrapper.content.oblast.addItem(key + " - " + hodnoty[sekce][key], key);
		
		if (key == oblast)
			nastavitOblast = true;
	}
	
	cz_nace.wrapper.content.sekceAktualni.rawValue = sekce;	
	
	if (nastavitOblast)
		cz_nace.wrapper.content.oblast.rawValue = oblast;
}

